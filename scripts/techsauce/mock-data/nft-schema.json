    "actions": [
      {
        "name": "check_in",
        "desc": "Attend to TechSauce Booth",
        "params": [],
        "when": "meta.GetBoolean('techsauce_booth') == false",
        "then": [
          "meta.SetFloat('points', meta.GetFloat('points') + 50.0)",
          "meta.SetBoolean('techsauce_booth', true)",
          "max_minor = Max(meta.GetFloat('minor_count')-5.0,-1.0)",
          "meta.SetFloat('minor_count', meta.GetFloat('minor_count')+Abs(max_minor))"
        ]
      },
      {
        "name": "attend_zone",
        "desc": "Attend to zone",
        "params": [
          {
            "name": "zone",
            "desc": "zone attribute",
            "data_type": "string",
            "required": true,
            "default_value": ""
          }
        ],
        "when": "meta.GetBoolean(params['zone'].GetString()) == false",
        "then": [
          "meta.SetFloat('points', meta.GetFloat('points') + 70.0)",
          "meta.SetBoolean(params['zone'].GetString(), true)",
          "max_minor = Max(meta.GetFloat('minor_count')-5.0,-1.0)",
          "meta.SetFloat('minor_count', meta.GetFloat('minor_count')+Abs(max_minor))"
        ]
      },
      {
        "name": "attend_exhibition",
        "desc": "Attend partner exhibition",
        "params": [
          {
            "name": "exhibition",
            "desc": "exhibition attribute",
            "data_type": "string",
            "required": true,
            "default_value": ""
          }
        ],
        "when": "meta.GetBoolean(params['exhibition'].GetString()) == false",
        "then": [
          "meta.SetFloat('points', meta.GetFloat('points') + 50.0)",
          "meta.SetBoolean(params['exhibition'].GetString(), true)",
          "meta.SetNumber('exhibition_count', meta.GetNumber('exhibition_count') + 1)",
          "actual_exhibition_count = meta.GetNumber('exhibition_count') + 1",
          "booth_count = Max(actual_exhibition_count-2.0,0.0)",
          "max_minor = Max(meta.GetFloat('minor_count')-5.0,0-booth_count)",
          "meta.SetFloat('minor_count', meta.GetFloat('minor_count')+Abs(max_minor))"
        ]
      },
      {
        "name": "attend_stage",
        "desc": "Attend stage event",
        "params": [
          {
            "name": "stage",
            "desc": "Stage attribute",
            "data_type": "string",
            "required": true,
            "default_value": ""
          }
        ],
        "when": "meta.GetBoolean(params['stage'].GetString()) == false",
        "then": [
          "max_ = Max(meta.GetFloat('stage_points')-400.0,-100.0)",
          "meta.SetFloat('stage_points', meta.GetFloat('stage_points')+Abs(max_))",
          "meta.SetFloat('points', meta.GetFloat('points')+Abs(max_))",
          "meta.SetBoolean(params['stage'].GetString(), true)",
          "meta.SetNumber('stage_count', meta.GetNumber('stage_count') + 1)"
        ]
      },
      {
        "name": "redeem_prize",
        "desc": "Redemtion points for prize",
        "params": [
          {
            "name": "stock",
            "desc": "redemtion stock",
            "data_type": "string",
            "required": true,
            "default_value": ""
          }
        ],
        "when": "meta.GetString('caption') != 'Staff' && meta.GetString('caption') != 'Organizer' && meta.GetBoolean('redemtion_used') == false && meta.GetFloat('points') >= meta.GetFloat('redemtion_'+ params['stock'].GetString())",
        "then": [
          "meta.SetBoolean('redemtion_used', true)",
          "meta.SetFloat('points', meta.GetFloat('points') - meta.GetFloat('redemtion_'+ params['stock'].GetString()))"
        ]
      },
      {
        "name": "claim_after_party",
        "desc": "Claim After Party",
        "params": [],
        "when": "(meta.GetString('caption') == 'VIP' || meta.GetString('caption') == 'Speaker'|| meta.GetString('caption') == 'Investor') && meta.GetBoolean('after_party_claimed') == false || meta.GetString('caption') != 'Staff' && meta.GetString('caption') != 'Organizer' && meta.GetBoolean('after_party_claimed') == false && meta.GetNumber('stage_count') >= 4 && meta.GetFloat('minor_count') >= 4.0",
        "then": [
          "meta.SetBoolean('after_party_claimed', true)"
        ]
      },
      {
        "name": "claim_voucher",
        "desc": "Voucher Redeemtion",
        "params": [],
        "when": "meta.GetString('caption') != 'Staff' && meta.GetString('caption') != 'Organizer' && meta.GetBoolean('voucher_claimed') == false",
        "then": [
          "meta.SetBoolean('voucher_claimed', true)"
        ]
      },
      {
        "name": "change_price",
        "desc": "Change Price of Redemtion",
        "params": [
          {
            "name": "stock",
            "desc": "redemtion stock",
            "data_type": "string",
            "required": true,
            "default_value": ""
          },
          {
            "name": "points",
            "desc": "New Point Value",
            "data_type": "number",
            "required": true,
            "default_value": ""
          }
        ],
        "when": "meta.GetString('caption') == 'Staff' || meta.GetString('caption') == 'Organizer'",
        "then": [
          "new_points = params['points'].GetNumber()",
          "meta.SetFloat(params['stock'].GetString(), 0.0 + new_points)"
        ],
        "allowed_actioner": "ALLOWED_ACTIONER_SYSTEM_ONLY"
      }
    ],
    "nft_attributes_value": []
