    "actions": [
      {
        "name": "check_in",
        "desc": "Check in booth event",
        "params": [],
        "when": "meta.GetBoolean('techsauce_booth') == false",
        "then": [
          "meta.SetBoolean('techsauce_booth', true)",
          "meta.SetNumber('points', meta.GetNumber('points') + 200)"
        ]
      },
      {
        "name": "attend_partner",
        "desc": "Attend partner event",
        "params": [
        {
            "name": "partner",
            "desc": "Partner attribute",
            "data_type": "string",
            "required" : true,
            "default_value": ""
        }
        ],
        "when": "meta.GetBoolean(params['partner'].GetString()) == false",
        "then": [
          "meta.SetNumber('points', meta.GetNumber('points') + 100)",
          "meta.SetBoolean(params['partner'].GetString(), true)",
          "meta.SetNumber('partner_count', meta.GetNumber('partner_count') + 1)"
        ]
      },
      {
        "name": "attend_stage",
        "desc": "Attend stage event",
        "params": [
        {
            "name": "stage",
            "desc": "Stage attribute",
            "data_type": "string",
            "required" : true,
            "default_value": ""
        }
        ],
        "when": "meta.GetBoolean(params['stage'].GetString()) == false",
        "then": [
          "meta.SetNumber('points', meta.GetNumber('points') + 100)",
          "meta.SetBoolean(params['stage'].GetString(), true)",
          "meta.SetNumber('stage_count', meta.GetNumber('stage_count') + 1)"
        ]
      },
      {
        "name": "attend_session",
        "desc": "Attend session event",
        "params": [
        {
            "name": "session",
            "desc": "Session attribute",
            "data_type": "string",
            "required" : true,
            "default_value": ""
        }
        ],
        "when": "meta.GetBoolean(params['session'].GetString()) == false",
        "then": [
          "meta.SetNumber('points', meta.GetNumber('points') + 100)",
          "meta.SetBoolean(params['session'].GetString(), true)",
          "meta.SetNumber('session_count', meta.GetNumber('session_count') + 1)"
        ]
      },
      {
        "name": "redeem_prize",
        "desc": "Redemtion points for prize",
        "params": [
          {
              "name": "stock",
              "desc": "redemtion stock",
              "data_type": "string",
              "required" : true,
              "default_value": ""
          }
        ],
        "when": "meta.GetNumber(params['stock'].GetString()) > 1 && meta.GetNumber('points') > meta.GetNumber('redemtion_'+ params['stock'].GetString())",
        "then": [
          "meta.SetNumber('points', meta.GetNumber('points') - meta.GetNumber('redemtion_'+ params['stock'].GetString()))",
          "meta.SetNumber(params['stock'].GetString(), meta.GetNumber(params['stock'].GetString()) - 1)"
        ]
      },
      {
        "name": "transfer",
        "desc": "Transfer",
        "params" : [
            {
                "name": "points",
                "desc": "Points",
                "data_type": "number",
                "required" : true,
                "default_value": "10"
            },
            {
                "name": "token_id",
                "desc": "Token Id",
                "data_type": "string",
                "required" : true,
                "default_value": ""
            }
        ],
        "when": "meta.GetNumber('points') > 0 && params['points'].GetNumber() > 0 ",
        "then": [
            "meta.TransferNumber('points',params['token_id'].GetString(),params['points'].GetNumber())"
        ]
      },
      {
        "name": "transform",
        "desc": "Transform",
        "params":[],
        "when": "meta.GetBoolean('techsauce_booth') == true && meta.GetNumber('partner_count') >= 4 && meta.GetNumber('stage_count') >= 5 && meta.GetNumber('session_count') >= 10  || meta.GetBoolean('vip') == true",
        "then": [
            "meta.SetBoolean('after_party', true)",
            "meta.SetImage(meta.ReplaceAllString(meta.GetImage(),'.jpeg','-t.jpeg'))"
        ]
      }
    ],
    "nft_attributes_value": []
  },
  "mint_authorization": "system"
